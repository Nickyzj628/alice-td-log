{
  "manifest_version": 3,
  "name": "Alice TD Monitor",
  "version": "1.0",
  "description": "Alice td log monitor",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "world": "MAIN",
      "js": [
        "content.js"
      ],
      "css": [
        "content.css",
        "app.css"
      ],
      "matches": [
        "https://test.alice-ai.com/*",
        "https://test2.alice-ai.com/*"
      ]
    }
  ]
}